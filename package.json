{
  "name": "@we-use/eslint-config",
  "type": "module",
  "version": "1.2.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --format cjs,esm --clean --dts",
    "build:inspector": "pnpm build && pnpx @eslint/config-inspector build",
    "dev": "pnpx @eslint/config-inspector --config eslint.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts:check": "tsc --noEmit",
    "gen:type": "tsx ./scripts/typegen.ts"
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.2",
    "@clack/prompts": "^0.7.0",
    "@eslint/config-inspector": "^0.4.7",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint-flat-config-utils": "^0.2.3",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-n": "^17.2.1",
    "globals": "^15.0.0",
    "local-pkg": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.4.6",
    "@types/eslint": "^8.56.9",
    "@types/node": "^20.12.7",
    "eslint": "^9.0.0",
    "eslint-typegen": "^0.2.4",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}